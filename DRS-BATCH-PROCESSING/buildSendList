#!/bin/bash -v
#
# Do the steps to getting files ready
RemoveDuplicateVolumes.sh > BuildList.txt
RemoveDepositedVolumePaths.sh BuildList.txt  UndepositedVolumes
 GetCumBuiltFileCounts.sh UndepositedVolumes > testFile
awk  -F'[|]' '{ if ( $3 < 200000) print $1 }' testFile | xargs -n 1 dirname | sort -u >  ../buildList.txt  
